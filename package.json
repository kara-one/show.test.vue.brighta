{
  "name": "brighta.test.vue",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "heroku-postbuild": "npm run build"
  },
  "dependencies": {
    "@babel/polyfill": "^7.4.4",
    "core-js": "^3.6.5",
    "vue": "^2.6.12",
    "vuetify": "^2.2.11",
    "vuex": "^3.4.0"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-plugin-vuex": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "babel-eslint": "^10.1.0",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^6.2.2",
    "sass": "^1.26.5",
    "sass-loader": "^8.0.2",
    "sass-resources-loader": "^2.1.0",
    "vue-cli-plugin-vuetify": "~2.0.7",
    "vue-template-compiler": "^2.6.11"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "es6": true,
      "browser": true,
      "node": true,
      "amd": true,
      "commonjs": true
    },
    "globals": {
      "process": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "ecmaVersion": 2020,
      "sourceType": "module",
      "parser": "babel-eslint"
    },
    "rules": {
      "eqeqeq": "off",
      "curly": "error",
      "quotes": [
        "error",
        "single"
      ],
      "semi": [
        "error"
      ],
      "no-extra-semi": "off",
      "indent": [
        "error",
        2
      ]
    }
  },
  "browserslist": [
    "> 0.2%",
    "last 5 versions",
    "not dead",
    "not ie <= 10"
  ]
}